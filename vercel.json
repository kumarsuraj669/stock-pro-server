{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    ,
    "functions": {
        "api/auth/login": {
          "runtime": "@vercel/node",
          "maxDuration": 30 
        }
    }
}
